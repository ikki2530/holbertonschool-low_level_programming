#include "holberton.h"
/**
 * rot13- encrypt a string rot13
 * @s: string to evaluate encrypt
 * Return: encrypting strign
 */
char *rot13(char *s)
{
	int i;
	int j;
	char enc[] = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ";
	char replace[] = "nopqrstuvwxyzabcdefghijklmNOPQRSTUVWXYZABCDEFGHIJKLM";

	for (i = 0; s[i] != '\0'; i++)
	{
		for (j = 0; j < 52; j++)
		{
			if (s[i] == enc[j])
			{
				s[i] = replace[j];
			}
		}
	}

	return (s);

}
